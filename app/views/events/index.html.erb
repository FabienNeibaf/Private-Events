<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h3>Upcoming Events</h3>
      <ul class="events">
        <% @upcoming.each do |event| %>
          <%= render event %>
        <% end %>
      </ul>
    </div>
    <div class="col-md-6">
      <h3>Past Events</h3>
      <ul class="events">
        <% @past.each do |event| %>
          <%= render event %>
        <% end %>
      </ul>
    </div>
  </div>
</div>
